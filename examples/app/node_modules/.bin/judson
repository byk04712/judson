#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andy/Downloads/judson/packages/judson-cli/bin/node_modules:/Users/andy/Downloads/judson/packages/judson-cli/node_modules:/Users/andy/Downloads/judson/packages/node_modules:/Users/andy/Downloads/judson/node_modules:/Users/andy/Downloads/node_modules:/Users/andy/node_modules:/Users/node_modules:/node_modules:/Users/andy/Downloads/judson/examples/app/node_modules:/Users/andy/Downloads/judson/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/andy/Downloads/judson/packages/judson-cli/bin/node_modules:/Users/andy/Downloads/judson/packages/judson-cli/node_modules:/Users/andy/Downloads/judson/packages/node_modules:/Users/andy/Downloads/judson/node_modules:/Users/andy/Downloads/node_modules:/Users/andy/node_modules:/Users/node_modules:/node_modules:/Users/andy/Downloads/judson/examples/app/node_modules:/Users/andy/Downloads/judson/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/judson-cli/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../packages/judson-cli/bin/index.js" "$@"
fi
