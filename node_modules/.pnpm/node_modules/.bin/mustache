#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules/mustache/bin/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules/mustache/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules:/Users/andy/Downloads/judson/node_modules:/Users/andy/Downloads/node_modules:/Users/andy/node_modules:/Users/node_modules:/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules/mustache/bin/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules/mustache/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules/mustache/bin/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules/mustache/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/registry.npmmirror.com+mustache@4.2.0/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules:/Users/andy/Downloads/judson/node_modules:/Users/andy/Downloads/node_modules:/Users/andy/node_modules:/Users/node_modules:/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules/mustache/bin/node_modules:/Users/andy/Downloads/judson/node_modules/.pnpm/node_modules/mustache/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mustache/bin/mustache" "$@"
else
  exec node  "$basedir/../mustache/bin/mustache" "$@"
fi
